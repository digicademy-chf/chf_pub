{
    "name": "digicademy/chf_pub",
    "version": "2.0.0",
    "type": "typo3-cms-extension",
    "description": "Create and manage blog and book data in TYPO3",
    "authors": [
        {
            "name": "Jonatan Jalle Steller",
            "email": "jonatan.steller@adwmainz.de",
            "role": "Developer",
            "homepage": "https://www.adwmainz.de"
        },
        {
            "name": "CHF Pub contributors"
        }
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://extensions.typo3.org/extension/chf_pub",
    "support": {
        "issues": "https://github.com/digicademy-chf/chf_pub/issues",
        "source": "https://github.com/digicademy-chf/chf_pub",
        "docs": "https://digicademy-chf.github.io/chf_pub"
    },
    "keywords": [
        "TYPO3",
        "Blog",
        "Book",
        "Volume",
        "Print",
        "Data",
        "Research",
        "Academia",
        "Academic"
    ],
    "require": {
        "typo3/cms-core": "^13",
        "typo3/cms-extbase": "^13",
        "typo3/cms-fluid": "^13",
        "typo3/cms-fluid-styled-content": "^13",
        "typo3/cms-rte-ckeditor": "^13",
        "digicademy/chf_base": "^2"
    },
    "suggest": {
        "digicademy/chf_bib": "Adds source relations",
        "digicademy/chf_gloss": "Provides a glossary component"
    },
    "autoload": {
        "psr-4": {
            "Digicademy\\CHFPub\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "chf_pub"
        }
    }
}